<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center|center_vertical"
    android:orientation="vertical"
    tools:context=".activities.MyHubActivity">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="241dp"
        android:layout_height="188dp"
        android:layout_marginTop="27dp"
        android:layout_marginBottom="93dp"
        app:layout_constraintBottom_toTopOf="@+id/btnViewBought"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/crescendo_pal_logo_circ" />

    <Button
        android:id="@+id/btnViewBought"
        android:layout_width="274dp"
        android:layout_height="77dp"
        android:layout_marginStart="66dp"
        android:layout_marginEnd="66dp"
        android:layout_marginBottom="50dp"
        android:text="View Bought Items"
        app:layout_constraintBottom_toTopOf="@+id/btnViewRented"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5" />

    <Button
        android:id="@+id/btnViewRented"
        android:layout_width="280dp"
        android:layout_height="81dp"
        android:layout_marginStart="65dp"
        android:layout_marginEnd="65dp"
        android:layout_marginBottom="173dp"
        android:text="View Rented Items"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnViewBought" />

</androidx.constraintlayout.widget.ConstraintLayout>

